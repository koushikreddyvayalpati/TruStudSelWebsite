{
  "name": "trustudsel-website",
  "version": 2,
  "buildCommand": "npm install && npm run build && cp public/phone4.png build/",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/(.+\\.(jpg|jpeg|png|gif|webp|svg|ico))$",
      "dest": "/$1"
    },
    {
      "src": "^/images/(.+\\.(jpg|jpeg|png|gif|webp|svg))$",
      "dest": "/images/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 